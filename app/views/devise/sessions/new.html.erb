<script type="text/javascript">
  mixpanel.track("Login Page View");
</script>
<div class="registration-background-wrapper">
  <%- if devise_mapping.omniauthable? %>
    <%- resource_class.omniauth_providers.each do |provider| %>
      <%= button_to "Sign in with Google", omniauth_authorize_path(resource_name, provider, {class: 'sign-in-form'}) %><br />
    <% end -%>
  <% end -%>
</div
